{
  "projectType": "application",
  "root": "apps/coreapp",
  "sourceRoot": "apps/coreapp/src",
  "prefix": "clientui",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:webpack-browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/coreapp",
        "index": "apps/coreapp/src/index.html",
        "main": "apps/coreapp/src/main.ts",
        "polyfills": "apps/coreapp/src/polyfills.ts",
        "tsConfig": "apps/coreapp/tsconfig.app.json",
        "assets": [
          "apps/coreapp/src/favicon.ico",
          "apps/coreapp/src/assets"
        ],
        "styles": [
          "node_modules/ng-zorro-antd/ng-zorro-antd.min.css",
          "apps/coreapp/src/styles.css"
        ],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/coreapp/webpack.config.js"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/coreapp/src/environments/environment.ts",
              "with": "apps/coreapp/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/coreapp/webpack.prod.config.js"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/angular:webpack-server",
      "configurations": {
        "production": {
          "browserTarget": "coreapp:build:production"
        },
        "development": {
          "browserTarget": "coreapp:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4202,
        "publicHost": "http://localhost:4202"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "coreapp:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/coreapp/src/**/*.ts",
          "apps/coreapp/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/coreapp"
      ],
      "options": {
        "jestConfig": "apps/coreapp/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
